@model Product

<div class="col-sm-4 col-lg-4 col-md-4">
    <div class="thumbnail" style="background-color:lavender;">
        <img src="@Model.ImageUrl" alt="" />
        <div class="caption">
            <h3 class="pull-right">@Model.Price.ToString("c")</h3>
            <h3>
                @Model.Game.Name
            </h3>
        </div>
    </div>
</div>
